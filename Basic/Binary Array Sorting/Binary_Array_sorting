//User function Template for Java

class Solution
{
    //Function to sort the binary array.
    static void binSort(int A[], int N)
    {
        
        int c=0;
        for(int i=0;i<N;i++)
            if(A[i]==0)
                c++;
        for(int i=0;i<c;i++)
            A[i]=0;
        for(int i=c;i<N;i++)
            A[i]=1;
    }
}

